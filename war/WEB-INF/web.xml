<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<listener>
		<listener-class>jipdbs.web.JIPDBSContextListener</listener-class>
	</listener>

	<servlet>
		<servlet-name>_ah_sessioncleanup</servlet-name>
		<servlet-class>com.google.apphosting.utils.servlet.SessionCleanupServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>serverlist</servlet-name>
		<servlet-class>jipdbs.web.ServerListServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>serverfetch</servlet-name>
		<servlet-class>jipdbs.web.ServerFetchServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>serversave</servlet-name>
		<servlet-class>jipdbs.web.ServerSaveServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>infoservlet</servlet-name>
		<servlet-class>jipdbs.web.InfoServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>xmlrpc</servlet-name>
		<servlet-class>jipdbs.xmlrpc.JIPDBSXmlRpcServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>search</servlet-name>
		<servlet-class>jipdbs.web.SearchServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>alias</servlet-name>
		<servlet-class>jipdbs.web.AliasServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>contactpost</servlet-name>
		<servlet-class>jipdbs.web.ContactPostServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>playerinfo</servlet-name>
		<servlet-class>jipdbs.web.PlayerInfoServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>truncate</servlet-name>
		<servlet-class>jipdbs.web.TruncateServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>mailhandler</servlet-name>
		<servlet-class>jipdbs.web.MailHandlerServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>httphandler</servlet-name>
		<servlet-class>jipdbs.http.JIPDBSHTTPHandler</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>keepalivehandler</servlet-name>
		<servlet-class>jipdbs.web.KeepAliveServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>duplication</servlet-name>
		<servlet-class>jipdbs.web.DuplicatesServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>flash</servlet-name>
		<servlet-class>jipdbs.web.FlashServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>_ah_sessioncleanup</servlet-name>
		<url-pattern>/_ah/sessioncleanup</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>flash</servlet-name>
		<url-pattern>/flash</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>mailhandler</servlet-name>
		<url-pattern>/_ah/mail/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>httphandler</servlet-name>
		<url-pattern>/app/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>playerinfo</servlet-name>
		<url-pattern>/playerinfo</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>serverlist</servlet-name>
		<url-pattern>/serverlist</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>contactpost</servlet-name>
		<url-pattern>/contactpost</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>infoservlet</servlet-name>
		<url-pattern>/infoservlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>serversave</servlet-name>
		<url-pattern>/admin/serversave</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>serverfetch</servlet-name>
		<url-pattern>/admin/serverfetch</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>truncate</servlet-name>
		<url-pattern>/admin/truncate</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>duplication</servlet-name>
		<url-pattern>/admin/duplication</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>xmlrpc</servlet-name>
		<url-pattern>/xmlrpc</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>search</servlet-name>
		<url-pattern>/search</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>alias</servlet-name>
		<url-pattern>/alias</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>alias</servlet-name>
		<url-pattern>/alias</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>keepalivehandler</servlet-name>
		<url-pattern>/cron/keepalive</url-pattern>
	</servlet-mapping>

	<security-constraint>
		<web-resource-collection>
			<web-resource-name>mail</web-resource-name>
			<url-pattern>/_ah/mail/*</url-pattern>
		</web-resource-collection>
		<web-resource-collection>
			<web-resource-name>session-cleanup</web-resource-name>
			<url-pattern>/_ah/sessioncleanup</url-pattern>
		</web-resource-collection>
		<web-resource-collection>
			<web-resource-name>admin</web-resource-name>
			<url-pattern>/admin/*</url-pattern>
		</web-resource-collection>
		<web-resource-collection>
			<web-resource-name>cron</web-resource-name>
			<url-pattern>/cron/*</url-pattern>
		</web-resource-collection>
		<auth-constraint>
			<role-name>admin</role-name>
		</auth-constraint>
	</security-constraint>

	<error-page>
		<error-code>403</error-code>
		<location>/error.jsp</location>
	</error-page>
	<error-page>
		<error-code>404</error-code>
		<location>/error.jsp</location>
	</error-page>

	<welcome-file-list>
		<welcome-file>search.jsp</welcome-file>
	</welcome-file-list>

	<session-config>
		<session-timeout>30</session-timeout>
	</session-config>
</web-app>
